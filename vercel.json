{
  "version": 2,
  "functions": {
    "api/*.go": {
      "runtime": "vercel-go@0.5.0"
    }
  },
  "routes": [
    {
      "src": "/health",
      "dest": "/api/health"
    },
    {
      "src": "/data",
      "dest": "/api/data"
    },
    {
      "src": "/api/v1/devices",
      "dest": "/api/devices"
    },
    {
      "src": "/api/v1/devices/(.*)",
      "dest": "/api/device?id=$1"
    }
  ]
}
